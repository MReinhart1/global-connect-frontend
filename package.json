    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-typescript-sort-keys": "^3.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
